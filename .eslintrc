{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "rules": {
    "strict": [
      2,
      "global"
    ],
    "no-underscore-dangle": 0,
    "key-spacing": [
      2,
      {
        "align": "value"
      }
    ],
    "quotes": [
      2,
      "single"
    ]
  }
}